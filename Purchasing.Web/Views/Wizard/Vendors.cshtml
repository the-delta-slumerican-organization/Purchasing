@model IEnumerable<Purchasing.Core.Domain.WorkgroupVendor>

@{
	ViewBag.StepName = "Vendor(s)";
	ViewBag.Previous = Url.Action("Accounts", "Wizard", new { id = ViewBag.WorkgroupId });
	ViewBag.Next = Url.Action("AddAddresses", "Wizard", new { id = ViewBag.WorkgroupId });
	ViewBag.ListPage = true;
}

	 @Html.Partial("_StatusBar", (int)ViewBag.WorkgroupId)
	 @Html.Partial("_Navigation")
     <nav class="col2">
	<ul class='navigation'>
		<li>@Html.ActionLink("Select KFS Vendor", "AddKfsVendor", new { id = ViewBag.WorkgroupId }, new { @class="button" })</li>
		<li>@Html.ActionLink("Add Vendor ", "addNewVendor", new { id = ViewBag.WorkgroupId }, new { @class = "button" })</li>
	</ul>
    </nav>


@Html.Partial("_VendorTable", Model)

@Html.Partial("_Navigation")
